<?xml version="1.0"?>
<extension key="com.drkhindol.anonymous" type="module">
  <file>anonymous</file>
  <name>Anonymous</name>
  <description>Simple extension to attribute anonymous contributions to the anonymous user. You need to have one and only contact with anonymous@contact.user primary email</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Dr. Khindol Madraimov</author>
    <email>khindol.madraimov@gmail.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">drhindol.wordpress.com</url>
    <url desc="Documentation">drhindol.wordpress.com</url>
    <url desc="Support">drhindol.wordpress.com</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2022-09-08</releaseDate>
  <version>1.0.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Anonymous</namespace>
    <format>22.05.2</format>
    <angularModule>crmAnonymous</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
  </mixins>
</extension>
